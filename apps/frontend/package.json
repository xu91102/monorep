{
  "name": "@monorep/frontend",
  "version": "1.0.0",
  "description": "Frontend application for the monorepo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "frontend",
    "vue",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@monorep/components": "workspace:*",
    "@monorep/utils": "workspace:*",
    "axios": "^1.13.2",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/tsconfig": "^0.5.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^3.2.2"
  }
}
